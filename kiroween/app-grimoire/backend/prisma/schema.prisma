generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Spell {
  id          String   @id @default(cuid())
  name        String
  incantation String
  ingredients String
  powerLevel  Int
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
